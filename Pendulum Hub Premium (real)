tossremainder then tf=0 else tf=tf-frame*math.floor(tf/frame)end end end)function PositiveAngle(aS)if aS>=0 then aS=0 end;return aS end;function NegativeAngle(aS)if aS<=0 then aS=0 end;return aS end;function Swait(aS)if aS==0 or aS==nil then ArtificialHB.Event:wait()else for aR=1,aS do ArtificialHB.Event:wait()end end end;function QuaternionFromCFrame(cf)local fJ,fK,fL,fM,fN,fO,fP,fQ,fR,fS,fT,fU=cf:components()local fV=fM+fQ+fU;if fV>0 then local aP=math.sqrt(1+fV)local fW=0.5/aP;return(fT-fR)*fW,(fO-fS)*fW,(fP-fN)*fW,aP*0.5 else local aR=0;if fQ>fM then aR=1 end;if fU>(aR==0 and fM or fQ)then aR=2 end;if aR==0 then local aP=math.sqrt(fM-fQ-fU+1)local fW=0.5/aP;return 0.5*aP,(fP+fN)*fW,(fS+fO)*fW,(fT-fR)*fW elseif aR==1 then local aP=math.sqrt(fQ-fU-fM+1)local fW=0.5/aP;return(fN+fP)*fW,0.5*aP,(fT+fR)*fW,(fO-fS)*fW elseif aR==2 then local aP=math.sqrt(fU-fM-fQ+1)local fW=0.5/aP;return(fO+fS)*fW,(fR+fT)*fW,0.5*aP,(fP-fN)*fW end end end;function QuaternionToCFrame(fX,fY,fZ,dH,f_,g0,g1)local g2,g3,g4=dH+dH,f_+f_,g0+g0;local g5,g6,g7=g1*g2,g1*g3,g1*g4;local g8=dH*g2;local g9=dH*g3;local ga=dH*g4;local gb=f_*g3;local gc=f_*g4;local gd=g0*g4;return CFrame.new(fX,fY,fZ,1-(gb+gd),g9-g7,ga+g6,g9+g7,1-(g8+gd),gc-g5,ga-g6,gc+g5,1-(g8+gb))end;function QuaternionSlerp(ge,gf,gg)local gh=ge[1]*gf[1]+ge[2]*gf[2]+ge[3]*gf[3]+ge[4]*gf[4]local gi,gj;if gh>=0.0001 then if 1-gh>0.0001 then local gk=ACOS(gh)local gl=1/SIN(gk)gi=SIN((1-gg)*gk)*gl;gj=SIN(gg*gk)*gl else gi=1-gg;gj=gg end else if 1+gh>0.0001 then local gk=ACOS(-gh)local gl=1/SIN(gk)gi=SIN((gg-1)*gk)*gl;gj=SIN(gg*gk)*gl else gi=gg-1;gj=gg end end;return ge[1]*gi+gf[1]*gj,ge[2]*gi+gf[2]*gj,ge[3]*gi+gf[3]*gj,ge[4]*gi+gf[4]*gj end;function Clerp(ge,gf,gg)local gm={QuaternionFromCFrame(ge)}local gn={QuaternionFromCFrame(gf)}local go,gp,gq=ge.x,ge.y,ge.z;local gr,gs,gt=gf.x,gf.y,gf.z;local gu=1-gg;return QuaternionToCFrame(gu*go+gg*gr,gu*gp+gg*gs,gu*gq+gg*gt,QuaternionSlerp(gm,gn,gg))end;function WeldParts(gv,gw)local gx=IT("ManualWeld")gx.Part0=gv;gx.Part1=gw;gx.C1=gw.CFrame:inverse()*gv.CFrame;gx.Parent=gv;return gx end;function NewSound(gy)local ID="rbxassetid://"..(gy.ID or 0)local bP=gy.PARENT or fc;local c4=gy.VOLUME or 0.5;local c5=gy.PITCH or 1;local cv=gy.LOOP or false;local gz=gy.MAXDISTANCE or 100;local gA=gy.EMITTERSIZE or 10;local gB=gy.PLAYING or true;local gC=gy.PLAYONREMOVE or false;local gD=gy.DOESDEBRIS or true;if ID~="rbxassetid://0"then local co=IT("Sound",bP)co.SoundId=ID;co.Volume=c4;co.Pitch=c5;co.Looped=cv;co.MaxDistance=gz;co.EmitterSize=gA;co.PlayOnRemove=gC;if gD==true and gB==true and cv==false then Debris:AddItem(co,co.TimeLength+5)end;if gB==true then co:Play()end;return co end end;function ManSlaughter(gE)if gE then local fc=gE:FindFirstChild("HumanoidRootPart")if fc then fc:Remove()end;local gF=gE:FindFirstChildOfClass("Humanoid")if gF then gF.BreakJointsOnDeath=false;gF.Health=0 end;if gE:FindFirstChild("R6Ragdoll")==nil and gE:FindFirstChild("R15Ragdoll")==nil then if gE:FindFirstChild("UpperTorso")then local gG=script.R15Ragdoll:Clone()gG.Parent=gE;gG.Disabled=false else local gG=script.R6Ragdoll:Clone()gG.Parent=gE;gG.Disabled=false end;local fe=gE:FindFirstChild("Torso")or gE:FindFirstChild("UpperTorso")if fe then NewSound({ID=146594648,PARENT=fe,VOLUME=1,PITCH=MRANDOM(8,12)/10,LOOP=false,MAXDISTANCE=75,EMITTERSIZE=15,PLAYING=true,PLAYONREMOVE=false,DOESDEBRIS=true})end end;gE:BreakJoints()if gE:FindFirstChild("Slaughtered")==nil then local gH=IT("Folder",gE)gH.Name="Slaughtered"else if gE:FindFirstChild("Slaughtered")then local fe=gE:FindFirstChild("Torso")or gE:FindFirstChild("UpperTorso")if fe then fe.RotVelocity=VT(0,MRANDOM(-25,25),0)end end end end end;function AreaOfEffect(gI,cR,gJ,gK)local gL={}for gM,gN in pairs(workspace:GetChildren())do if gN:FindFirstChildOfClass("Humanoid")then local gO=gN:FindFirstChildOfClass("Humanoid")local fc=gO.Torso;if fc then if(fc.Position-gI).Magnitude<=cR then if gK==true then if gN~=f9 then table.insert(gL,{gN,gO,fc})end else if gN~=f9 then ManSlaughter(gN)end;local gP=IT("BodyVelocity")gP.MaxForce=gP.MaxForce*500;gP.Velocity=CF(gI,fc.Position+VT(0,0.5,0)).lookVector*gJ;gP.Parent=fc;Debris:AddItem(gP,0.05)end end end end end;if gK==true then return gL end end;function CreatePart(bW,bP,bX,bY,bZ,b_,c0,c1,c2)local c3=IT("Part")c3.formFactor=bW;c3.Reflectance=bY;c3.Transparency=bZ;c3.CanCollide=false;c3.Locked=true;c3.Anchored=true;if c2==false then c3.Anchored=false end;c3.BrickColor=BRICKC(tostring(b_))c3.Name=c0;c3.Size=c1;c3.Position=fc.Position;c3.Material=bX;c3:BreakJoints()c3.Parent=bP;return c3 end;function Raycast(cP,cQ,cR,cS)return workspace:FindPartOnRay(Ray.new(cP,cQ.unit*cR),cS)end;function CameraShake(gI,cR,gQ,gR)for gM,gN in pairs(workspace:GetChildren())do if gN:FindFirstChildOfClass("Humanoid")then local gO=gN:FindFirstChildOfClass("Humanoid")local fc=gO.Torso;if fc and game.Players:GetPlayerFromCharacter(gN)then if(fc.Position-gI).Magnitude<=cR then if gN:FindFirstChild("CamShake")then Debris:AddItem(gN:FindFirstChild("CamShake"),0.05)end;local gS=script.CamShake:Clone()gS.Parent=gN;local gT=IT("NumberValue",gS)gT.Name="Timer"gT.Value=gR;local gU=IT("NumberValue",gS)gU.Name="Shake"gU.Value=gQ;gS.Disabled=false;Debris:AddItem(gS,15)end end end end end;local gV=f8.Helmet;gV.Parent=f9;local gW=fd.face;gW.Texture="rbxassetid://559113097"gV.CFrame=fd.CFrame*CF(0,0.5,0)gV.Anchored=false;WeldParts(fd,gV)local gX=f8.Bazooka;gX.Parent=f9;gX.Anchored=false;local gY=IT("Motor6D")gY.Part0=ff;gY.Part1=gX;gY.C0=CF(0,-1,-0.5)*ANGLES(RAD(-90),0,0)*CF(0,0,1.85)local gZ=gY.C0;gY.Parent=ff;for aR=1,15 do local gW=CreatePart(3,f9,"Fabric",0,0+(aR-1)/15.2,"Dark stone grey","FaceGradient",VT(1.01,0.58,1.01),false)gW.Color=C3(0,0,0)fd:FindFirstChildOfClass("SpecialMesh"):Clone().Parent=gW;gW.CFrame=fd.CFrame*CF(0,0.3-(aR-1)/35,0)WeldParts(fd,gW)end;local g_=f8.Missile;g_.Parent=nil;local h0=f8.ControlsGUI;h0.Parent=fa.PlayerGui;h0.ToggleButton.MouseButton1Click:Connect(function()if h