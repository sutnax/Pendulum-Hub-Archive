()local fV=fM+fQ+fU;if fV>0 then local aP=math.sqrt(1+fV)local fW=0.5/aP;return(fT-fR)*fW,(fO-fS)*fW,(fP-fN)*fW,aP*0.5 else local aR=0;if fQ>fM then aR=1 end;if fU>(aR==0 and fM or fQ)then aR=2 end;if aR==0 then local aP=math.sqrt(fM-fQ-fU+1)local fW=0.5/aP;return 0.5*aP,(fP+fN)*fW,(fS+fO)*fW,(fT-fR)*fW elseif aR==1 then local aP=math.sqrt(fQ-fU-fM+1)local fW=0.5/aP;return(fN+fP)*fW,0.5*aP,(fT+fR)*fW,(fO-fS)*fW elseif aR==2 then local aP=math.sqrt(fU-fM-fQ+1)local fW=0.5/aP;return(fO+fS)*fW,(fR+fT)*fW,0.5*aP,(fP-fN)*fW end end end;function QuaternionToCFrame(fX,fY,fZ,dH,f_,g0,g1)local g2,g3,g4=dH+dH,f_+f_,g0+g0;local g5,g6,g7=g1*g2,g1*g3,g1*g4;local g8=dH*g2;local g9=dH*g3;local ga=dH*g4;local gb=f_*g3;local gc=f_*g4;local gd=g0*g4;return CFrame.new(fX,fY,fZ,1-(gb+gd),g9-g7,ga+g6,g9+g7,1-(g8+gd),gc-g5,ga-g6,gc+g5,1-(g8+gb))end;function QuaternionSlerp(ge,gf,gg)local gh=ge[1]*gf[1]+ge[2]*gf[2]+ge[3]*gf[3]+ge[4]*gf[4]local gi,gj;if gh>=0.0001 then if 1-gh>0.0001 then local gk=math.acos(gh)local gl=1/math.sin(gk)gi=math.sin((1-gg)*gk)*gl;gj=math.sin(gg*gk)*gl else gi=1-gg;gj=gg end else if 1+gh>0.0001 then local gk=math.acos(-gh)local gl=1/math.sin(gk)gi=math.sin((gg-1)*gk)*gl;gj=math.sin(gg*gk)*gl else gi=gg-1;gj=gg end end;return ge[1]*gi+gf[1]*gj,ge[2]*gi+gf[2]*gj,ge[3]*gi+gf[3]*gj,ge[4]*gi+gf[4]*gj end;function clerp(ge,gf,gg)local gm={QuaternionFromCFrame(ge)}local gn={QuaternionFromCFrame(gf)}local go,gp,gq=ge.x,ge.y,ge.z;local gr,gs,gt=gf.x,gf.y,gf.z;local gu=1-gg;return QuaternionToCFrame(gu*go+gg*gr,gu*gp+gg*gs,gu*gq+gg*gt,QuaternionSlerp(gm,gn,gg))end;local j6=false;Humanoid.FreeFalling:connect(function(j7)if j7 then j6=true else j6=false end end)function Vapor()end;function Vape()if iA==true and iz>5 then iy=true;for aR=0,3,0.01 do ir=true;RA_Weld.C0=clerp(RA_Weld.C0,c_new(1.5,0.5,0.1)*c_angles(math.rad(20),math.rad(0),math.rad(20)),0.01)LA_Weld.C0=clerp(LA_Weld.C0,c_new(-1.5,0.5,0.1)*c_angles(math.rad(20),math.rad(0),math.rad(-20)),0.01)LL_Weld.C0=clerp(LL_