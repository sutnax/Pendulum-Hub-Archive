("ArtificialHB")frame=fu;tf=0;allowframeloss=false;tossremainder=false;lastframe=tick()script.ArtificialHB:Fire()game:GetService("RunService").Heartbeat:connect(function(aP,aQ)tf=tf+aP;if tf>=frame then if allowframeloss then ArtificialHB:Fire()lastframe=tick()else for aR=1,math.floor(tf/frame)do ArtificialHB:Fire()end;lastframe=tick()end;if tossremainder then tf=0 else tf=tf-frame*math.floor(tf/frame)end end end)function PositiveAngle(aS)if aS>=0 then aS=0 end;return aS end;function NegativeAngle(aS)if aS<=0 then aS=0 end;return aS end;function Swait(aS)if aS==0 or aS==nil then ArtificialHB.Event:wait()else for aR=1,aS do ArtificialHB.Event:wait()end end end;function QuaternionFromCFrame(cf)local fJ,fK,fL,fM,fN,fO,fP,fQ,fR,fS,fT,fU=cf:components()local fV=fM+fQ+fU;if fV>0 then local aP=math.sqrt(1+fV)local fW=0.5/aP;return(fT-fR)*fW,(fO-fS)*fW,(fP-fN)*fW,aP*0.5 else local aR=0;if fQ>fM then aR=1 end;if fU>(aR==0 and fM or fQ)then aR=2 end;if aR==0 then local aP=math.sqrt(fM-fQ-fU+1)local fW=0.5/aP;return 0.5*aP,(fP+fN)*fW,(fS+fO)*fW,(fT-fR)*fW elseif aR==1 then local aP=math.sqrt(fQ-fU-fM+1)local fW=0.5/aP;return(fN+fP)*fW,0.5*aP,(fT+fR)*fW,(fO-fS)*fW elseif aR==2 then local aP=math.sqrt(fU-fM-fQ+1)local fW=0.5/aP;return(fO+fS)*fW,(fR+fT)*fW,0.5*aP,(fP-fN)*fW end end end;function QuaternionToCFrame(fX,fY,fZ,dH,f_,g0,g1)local g2,g3,g4=dH+dH,f_+f_,g0+g0;local g5,g6,g7=g1*g2,g1*g3,g1*g4;local g8=dH*g2;local g9=dH*g3;local ga=dH*g4;local gb=f_*g3;local gc=f_*g4;local gd=g0*g4;return CFrame.new(fX,fY,fZ,1-(gb+gd),g9-g7,ga+g6,g9+g7,1-(g8+gd),gc-g5,ga-g6,gc+g5,1-(g8+gb))end;function QuaternionSlerp(ge,gf,gg)local gh=ge[1]*gf[1]+ge[2]*gf[2]+ge[3]*gf[3]+ge[4]*gf[4]local gi,gj;if gh>=0.0001 then if 1-gh>0.0001 then local gk=ACOS(gh)local gl=1/SIN(gk)gi=SIN((1-gg)*gk)*gl;gj=SIN(gg*gk)*gl else gi=1-gg;gj=gg end else if 1+gh>0.0001 then local gk=ACOS(-gh)local gl=1/SIN(gk)gi=SIN((gg-1)*gk)*gl;gj=SIN(gg*gk)*gl else gi=gg-1;gj=gg end end;return ge[1]*gi+gf[1]*gj,ge[2]*gi+gf[2]*gj,ge[3]*gi+gf[3]*gj,ge[4]*gi+gf[4]*gj end;function Clerp(ge,gf,gg)local gm={QuaternionFromCFrame(ge)}local gn={QuaternionFromCFrame(gf)}local go,gp,gq=ge.x,ge.y,ge.z;local gr,gs,gt=gf.x,gf.y,gf.z;local gu=1-gg;return QuaternionToCFrame(gu*go+gg*gr,gu*gp+gg*gs,gu*gq+gg*gt,QuaternionSlerp(gm,gn,gg))end;function WeldParts(gv,gw)local gx=IT("ManualWeld")gx.Part0=gv;gx.Part1=gw;gx.C1=gw.CFrame:inverse()*gv.CFrame;gx.Parent=gv;return gx end;function NewSound(gy)local ID="rbxassetid://"..(gy.ID or 0)local bP=gy.PARENT or fc;local c4=gy.VOLUME or 0.5;local c5=gy.PITCH or 1;local cv=gy.LOOP or false;local gz=gy.MAXDISTANCE or 100;local gA=gy.EMITTERSIZE or 10;local gB=gy.PLAYING or true;local gC=gy.PLAYONREMOVE or false;local gD=gy.DOESDEBRIS or true;if ID~="rbxassetid://0"then local co=IT("Sound",bP)co.SoundId=ID;co.Volume=c4;co.Pitch=c5;co.Looped=cv;co.MaxDistance=gz;co.EmitterSize=gA;co.PlayOnRemove=gC;if gD==true and gB==true and cv==false then Debris:AddItem(co,co.TimeLength+5)end;if gB==true then co:Play()end;return co end end;function OofOuchBlood(hq,hr,hs)local ht=CreatePart(3,Effects,"Granite",0,1,BRICKC("Pearl"),"Blood",VT(0,0,0),true)ht.CFrame=CF(hq,hr)local hu=f8.Blood:Clone()hu.Parent=ht;hu:Emit(hs)Debris:AddItem(ht,2)end;function AttachmentCFrame(gv)return gv.Parent.CFrame*CF(gv.Position)end;function CastProperRay(cT,cU,cV,bi)local cQ=CF(cT,cU).lookVector;return Raycast(cT,cQ,cV,bi)end;function ManSlaughter(gE)end;function ApplyDamageTo(fb,hv)if fb.Health>=0 then if fb.Health<=0 then ManSlaughter(fb.Parent)end end end;function AreaOfEffect(gI,cR,hw,hx,gK)local gL={}for gM,gN in pairs(workspace:GetChildren())do if gN:FindFirstChildOfClass("Humanoid")and gN~=f9 then local gO=gN:FindFirstChildOfClass("Humanoid")local fc=gO.Torso;if fc then if(fc.Position-gI).Magnitude<=cR then if gK==true then table.insert(gL,{gN,gO,fc})else ApplyDamageTo(gO,MRANDOM(hw,hx))end end end end end;if gK==true then return gL end end;function CreateMesh(bO,bP,bQ,bR,bS,bT,bU)local bV=IT(bO)if bO=="SpecialMesh"then bV.MeshType=bQ;if bR~="nil"and bR~=""then bV.MeshId="http://www.roblox.com/asset/?id="..bR end;if bS~="nil"and bS~=""then bV.TextureId="http://www.roblox.com/asset/?id="..bS end end;bV.Offset=bU or VT(0,0,0)bV.Scale=bT;bV.Parent=bP;return bV end;function CreatePart(bW,bP,bX,bY,bZ,b_,c0,c1,c2)local c3=IT("Part")c3.formFactor=bW;c3.Reflectance=bY;c3.Transparency=bZ;c3.CanCollide=false;c3.Locked=true;c3.Anchored=true;if c2==false then c3.Anchored=false end;c3.BrickColor=BRICKC(tostring(b_))c3.Name=c0;c3.Size=c1;c3.Position=fc.Position;c3.Material=bX;c3:BreakJoints()c3.Parent=bP;return c3 end;function Raycast(cP,cQ,cR,gy)local gy=type(gy)=="table"and gy or{gy}return game:GetService("Workspace"):FindPartOnRayWithIgnoreList(Ray.new(cP,cQ*cR),gy)end;function CameraShake(gI,cR,gQ,gR)for gM,gN in pairs(workspace:GetChildren())do if gN:FindFirstChildOfClass("Humanoid")then local gO=gN:FindFirstChildOfClass("Humanoid")local fc=gO.Torso;if fc and game.Players:GetPlayerFromCharacter(gN)then if(fc.Position-gI).Magnitude<=cR then if gN:FindFirstChild("CamShake")then Debris:AddItem(gN:FindFirstChild("CamShake"),0.05)end;local gS=f8.CamShake:Clone()gS.Parent=gN;local gT=IT("NumberValue",gS)gT.Name="Timer"gT.Value=gR;local gU=IT("NumberValue",gS)gU.Name="Shake"gU.Value=gQ;gS.Disabled=false;Debris:AddItem(gS,15)end end end end end;local hy=false;local hz={{2598781800,1.75},{373410757,2},{327466331,2.7},{193170682,2.2},{2496169936,2.5},{1703313447,2.3},{2884982022,4.25},{3397313642,3},{601814175,2.2}}local h9=1;local hA=f8.Molotov;hA.Parent=nil;local hB=f8.Missile;hB.Parent=nil;local hC=f8.Bullet;hC.Parent=nil;local hD=f8.FrontWheel;local hE=f8.BackWheel;local hF=hD.CFrame:inverse()*hm.CFrame;local hG=hE.CFrame:inverse()*hm.CFrame;local hH=hD:Clone()hH.Parent=nil;hH.CanCollide=true;hH.Anchored=false;local hI={{hD,hF},{hE,hG}}local hJ={}local hK=IT("Model",f9)hK.Name="MotorThings"for aR=1,#hI do hI[aR][1].Anchored=false;hI[aR][1].Parent=hK;local hL=IT("Motor6D"